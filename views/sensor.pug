-var dictionary = data.dictionary
doctype html
html(lang='en')
  head
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0')
    title
    script(src='//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js')
    script(src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js')
    link(href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css', rel='stylesheet')
    link(href='main.css', rel='stylesheet', type='text/css')
    link(rel='stylesheet', href='https://fonts.googleapis.com/css?family=Lato|Open+Sans')
  body
    .container
      h2.moverizquierda= dictionary[data.estacion_id]
      .row(style='margin-right= 0px;')
        button.btn.btn-primary(type='button', onclick='getData("estacion");')#atras Atras
      #contenedor
          label(for='inputPassword') ESTACIÃ“N
          select(onchange='getData("sensores/"+this.options[this.selectedIndex].value);').form-control.selectsensor
              if(data.estacion_id=='la_boca')
                option(value='la_boca', selected) La Boca
              else
                option(value='la_boca') La Boca
              if (data.estacion_id=='centenario')
                option(value='centenario', selected) Centenario
              else
                option(value='centenario') Centenario
              if(data.estacion_id=='cordoba')
                option(value='cordoba', selected) Cordoba
              else
                option(value='cordoba') Cordoba
      .col-md-12
          #contacts
            table.table.table-bordered
              thead
                tr
                  th.sort(data-sort='name') Nombre
              tbody
                for sensor in data.sensores
                  tr
                    td
                      a(href="#", onclick='getData("mediciones/'+sensor.Id+'/'+data.estacion_id+'");')= (dictionary[sensor.Id])[0]